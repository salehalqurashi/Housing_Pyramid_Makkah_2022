عدد اناث = 
CALCULATE(
    SUM('الهرم-السكاني-عدد-السكان-الإجم'[تعداد السكان]),
    'الهرم-السكاني-عدد-السكان-الإجم'[الجنس] = "إناث"
)

كويري لحساب عدد الاناث

عدد ذكور = 
CALCULATE(
    SUM('الهرم-السكاني-عدد-السكان-الإجم'[تعداد السكان]),
    'الهرم-السكاني-عدد-السكان-الإجم'[الجنس] = "ذكور"
)

كويري لحساب عدد الذكور

